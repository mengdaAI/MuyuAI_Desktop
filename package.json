{
    "name": "muyu-ai-desktop",
    "productName": "muyu",
    "version": "0.2.4",
    "description": "幕语实时语音字幕",
    "main": "src/index.js",
    "scripts": {
        "start": "npm run build:renderer && node ./node_modules/electron/cli.js .",
        "dev": "npm-run-all -p watch:renderer dev:main",
        "dev:main": "ENABLE_ELECTRON_RELOAD=false NODE_ENV=development nodemon --watch src --ext js,ts,tsx,json --exec \"node ./node_modules/electron/cli.js .\"",
        "package": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --dir",
        "make": "npm run build:renderer && electron-forge make",
        "build": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --config electron-builder.yml --publish never",
        "build:win": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --win --x64 --publish never",
        "publish": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --config electron-builder.yml --publish always",
        "lint": "eslint --ext .ts,.tsx,.js .",
        "postinstall": "node ./node_modules/electron-builder/out/cli/cli.js install-app-deps",
        "build:renderer": "node build.js",
        "build:all": "npm run build:renderer",
        "watch:renderer": "node build.js --watch",
        "generate-icons": "node scripts/generate-icons.js",
        "package:mac": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --mac --dir",
        "package:win": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --win --dir",
        "build:mac:dmg": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --mac dmg",
        "build:win:nsis": "npm run build:all && node ./node_modules/electron-builder/out/cli/cli.js --win nsis"
    },
    "keywords": [
        "muyu",
        "cluely",
        "ai assistant",
        "real-time",
        "live summary",
        "contextual ai"
    ],
    "author": {
        "name": "Muyu Team"
    },
    "license": "GPL-3.0",
    "dependencies": {
        "@anthropic-ai/sdk": "^0.56.0",
        "@deepgram/sdk": "^4.9.1",
        "@google/genai": "^1.8.0",
        "@google/generative-ai": "^0.24.1",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-slot": "^1.2.4",
        "@radix-ui/react-tooltip": "^1.2.8",
        "axios": "^1.10.0",
        "better-sqlite3": "^9.6.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cors": "^2.8.5",
        "dotenv": "^17.0.0",
        "electron-squirrel-startup": "^1.0.1",
        "electron-store": "^8.2.0",
        "electron-updater": "^6.6.2",
        "express": "^4.18.2",
        "input-otp": "^1.4.2",
        "jsonwebtoken": "^9.0.2",
        "keytar": "^7.9.0",
        "lucide-react": "^0.555.0",
        "node-fetch": "^2.7.0",
        "openai": "^4.70.0",
        "portkey-ai": "^1.10.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hot-toast": "^2.5.2",
        "sharp": "^0.34.2",
        "tailwind-merge": "^3.4.0",
        "validator": "^13.11.0",
        "wait-on": "^8.0.3",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@electron/fuses": "^1.8.0",
        "@electron/notarize": "^2.5.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "autoprefixer": "^10.4.22",
        "chokidar": "^4.0.3",
        "electron": "^30.5.1",
        "electron-builder": "^26.0.12",
        "electron-reloader": "^1.2.3",
        "esbuild": "^0.25.5",
        "eslint": "^9.39.1",
        "nodemon": "^3.1.11",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "prettier": "^3.6.2",
        "tailwindcss": "^3.4.18"
    },
    "optionalDependencies": {
        "electron-liquid-glass": "^1.0.1"
    }
}
